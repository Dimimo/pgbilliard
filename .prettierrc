{
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "plugins": ["prettier-plugin-blade", "prettier-plugin-tailwindcss"],
    "tailwindConfig": "tailwind.config.js",
    "overrides": [
        {
            "files": [
                "*.blade.php"
            ],
            "options": {
                "parser": "blade"
            }
        }
    ]
}
